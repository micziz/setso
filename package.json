{
  "name": "setso",
  "license": "MIT",
  "version": "0.2.0",
  "description": "A small library to compile markdown to html in a ordered format.",
  "main": "./bundle.js",
  "type": "module",
  "bin": {
    "setso": "./bundle.js"
  },
  "scripts": {
    "dev": "pnpm build-fast && node .",
    "build": "pnpm prepare && rollup -c",
    "build-fast": "rollup -c",
    "go": "pnpm build && pnpm publish",
    "lint": "eslint src --fix",
    "check": "prettier src --check",
    "format": "prettier src --write",
    "prepare": "pnpm format && pnpm lint"
  },
  "keywords": [
    "ssg",
    "minimal",
    "micromark"
  ],
  "author": "micziz",
  "dependencies": {
    "fs-extra": "^11.1.0",
    "micromark": "^3.1.0"
  },
  "devDependencies": {
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "prettier": "^2.8.3",
    "rollup": "^3.10.1"
  }
}
